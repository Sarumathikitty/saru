n=str(input())
count=0
for i in range(len(n)):
	if(n[i]=='a'):
		count=count+1
	elif(n[i]=='e'):
		count=count+1
	elif(n[i]=='i'):
		count=count+1
	elif(n[i]=='o'):
		count=count+1
	elif(n[i]=='u'):
		count=count+1
	elif(n[i]=='A'):
		count=count+1
	elif(n[i]=='E'):
		count=count+1
	elif(n[i]=='I'):
		count=count+1
	elif(n[i]=='o'):
		count=count+1
	elif(n[i]=='U'):
		count=count+1
if(count>0):
	print("yes")
else:
	print("no")  
        
